<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hp.attendance_master.OurTeamFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="45dp"
        android:orientation="vertical"
        android:padding="18dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:id="@+id/rln">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_margin="2dp"
                android:src="@drawable/nitesh" />

            <TextView
                android:id="@+id/textView_ns"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:text="Nitesh Sahni"
                android:textAlignment="center"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="30dp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView_ns"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toEndOf="@+id/imageView3"
                android:layout_toRightOf="@+id/imageView3"
                android:orientation="horizontal"
                android:padding="10dp">

                <Button
                    android:id="@+id/button_fn"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView5"
                    android:layout_centerHorizontal="true"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/facebook"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button_tn"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/button_fav"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button_fav"
                    android:layout_toRightOf="@+id/button_fav"
                    android:layout_weight="1"
                    android:background="@drawable/twitter"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button_gn"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/button2"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button2"
                    android:layout_toRightOf="@+id/button2"
                    android:layout_weight="1"
                    android:background="@drawable/google"
                    android:padding="5dp" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_margin="2dp"
                android:src="@drawable/profilephoto" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_toLeftOf="@+id/imageView4"
                android:layout_toStartOf="@+id/imageView4"
                android:text="Ankit Varshney"
                android:textAlignment="center"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="25dp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView5"
                android:layout_alignParentBottom="true"
                android:layout_toStartOf="@+id/imageView4"
                android:orientation="horizontal"
                android:padding="10dp">

                <Button
                    android:id="@+id/button_f_av"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView5"
                    android:layout_centerHorizontal="true"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/facebook"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button_tav"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/button_fav"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button_fav"
                    android:layout_toRightOf="@+id/button_fav"
                    android:layout_weight="1"
                    android:background="@drawable/twitter"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button_gav"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/button2"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button2"
                    android:layout_toRightOf="@+id/button2"
                    android:layout_weight="1"
                    android:background="@drawable/google"
                    android:padding="5dp" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">


            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_margin="2dp"
                android:src="@drawable/akshat" />

            <TextView
                android:id="@+id/textView_ak"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/imageView5"
                android:text="Akshat"
                android:textAlignment="center"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textSize="30dp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView_ak"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toEndOf="@+id/imageView5"
                android:layout_toRightOf="@+id/imageView5"
                android:orientation="horizontal"
                android:padding="10dp">

                <Button
                    android:id="@+id/button_fak"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView5"
                    android:layout_centerHorizontal="true"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/facebook"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button_tak"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/button_fav"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button_fav"
                    android:layout_toRightOf="@+id/button_fav"
                    android:layout_weight="1"
                    android:background="@drawable/twitter"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button_gak"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/button2"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button2"
                    android:layout_toRightOf="@+id/button2"
                    android:layout_weight="1"
                    android:background="@drawable/google"
                    android:padding="5dp" />
            </LinearLayout>


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_margin="2dp"
                android:src="@drawable/profilephoto" />

            <TextView
                android:id="@+id/textView_rv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_toStartOf="@+id/imageView6"
                android:text="Rishabh Vidhani"
                android:textAlignment="center"
                android:textSize="23dp"
                android:textAppearance="?android:attr/textAppearanceLarge" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/textView_rv"
                android:layout_alignBottom="@+id/imageView6"
                android:layout_alignParentBottom="true"
                android:layout_toStartOf="@+id/imageView6"
                android:orientation="horizontal"
                android:padding="10dp">

                <Button
                    android:id="@+id/button_fr"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/imageView5"
                    android:layout_centerHorizontal="true"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:background="@drawable/facebook"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button2"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/button_fav"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button_fav"
                    android:layout_toRightOf="@+id/button_fav"
                    android:layout_weight="1"
                    android:background="@drawable/twitter"
                    android:padding="5dp" />

                <Button
                    android:id="@+id/button3"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/button2"
                    android:layout_margin="10dp"
                    android:layout_toEndOf="@+id/button2"
                    android:layout_toRightOf="@+id/button2"
                    android:layout_weight="1"
                    android:background="@drawable/google"
                    android:padding="5dp" />
            </LinearLayout>

        </RelativeLayout>
    </LinearLayout>

</FrameLayout>
